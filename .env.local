# .env.example
# 환경변수 예시 파일 - 실제 값은 입력하지 말고 형식만 보여주기

# 환경 구분
NODE_ENV=development
NEXT_PUBLIC_ENV=development

# API 서버
NEXT_PUBLIC_API_URL=https://dev-api.fablink.com
API_URL=https://dev-api.fablink.com

# 인증 관련
NEXTAUTH_SECRET=your-secret-key
NEXTAUTH_URL=https://dev.fablink.com

# AWS 설정
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET=fablink-dev-uploads

# 디버그 모드
DEBUG=true
NEXT_PUBLIC_DEBUG=true