# .env.example
# 환경변수 템플릿 파일

# 환경 구분
NODE_ENV=development
NEXT_PUBLIC_ENV=local

# API 서버
NEXT_PUBLIC_API_URL=http://localhost:8000
API_URL=http://localhost:8000

# Lambda 함수 URL
NEXT_PUBLIC_GEN_SKETCH_URL=https://6ydwjfj2xb.execute-api.ap-northeast-2.amazonaws.com/Fablink-dev/gen-sketch
NEXT_PUBLIC_GEN_SKETCH_PET_URL=https://6ydwjfj2xb.execute-api.ap-northeast-2.amazonaws.com/Fablink-dev/gen-sketch-fat

# 인증 관련
NEXTAUTH_SECRET=your-secret-key
NEXTAUTH_URL=http://localhost:3000

# AWS 설정
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET=your-s3-bucket-name
NEXT_PUBLIC_AWS_REGION=ap-northeast-2
NEXT_PUBLIC_AWS_S3_BUCKET=your-s3-bucket-name

# 미들웨어 토글 (정적 호스팅에서는 false 권장)
NEXT_PUBLIC_ENABLE_MIDDLEWARE=false

# 디버그 모드
DEBUG=true
NEXT_PUBLIC_DEBUG=true

# 개발 환경 설정
LOG_LEVEL=info
ENABLE_EXPERIMENTAL_FEATURES=true
MOCK_EXTERNAL_APIS=false
